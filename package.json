{
    "name": "grovejs",
    "version": "1.0.0",
    "description": "Lightweight library for tree structure",
    "main": "dist/grove.js",
    "module": "dist/grove.esm.js",
    "scripts": {
        "test": "npm run test:lint && npm run test:spell && npm run test:unit",
        "test:unit": "jest",
        "test:spell": "cspell 'src/**/*.js' 'dist/**' 'test/**'",
        "test:lint": "eslint src/**",
        "doc": "npm run doc:lint && npm run doc:md & npm run doc:html",
        "doc:lint": "documentation lint src/index.js",
        "doc:md": "documentation build src/index.js -f md -o docs/markdown/index.md",
        "doc:html": "documentation build src/index.js -f html -o docs/html",
        "build": "rollup -c"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tuplle/grovejs.git"
    },
    "keywords": [
        "tree",
        "trie"
    ],
    "files": [
        "dist",
        "LICENSE",
        "README.md",
        "package.json",
        "!**/*.spec.*",
        "!**/test"
    ],
    "author": "Milan Mladoniczky",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tuplle/grovejs/issues"
    },
    "homepage": "https://github.com/tuplle/grovejs#readme",
    "type": "module",
    "devDependencies": {
        "@eslint/js": "~9.14.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-terser": "~0.4.4",
        "@rollup/plugin-typescript": "^12.1.1",
        "cspell": "~8.16.0",
        "documentation": "~14.0.3",
        "eslint": "~9.14.0",
        "globals": "~15.12.0",
        "jest": "~29.7.0",
        "jest-sonar-reporter": "~2.0.0",
        "rollup": "~4.26.0",
        "rollup-plugin-delete": "~2.1.0",
        "tslib": "^2.8.1",
        "typescript": "^5.6.3"
    },
    "jest": {
        "verbose": true,
        "collectCoverage": true,
        "testResultsProcessor": "jest-sonar-reporter"
    }
}
